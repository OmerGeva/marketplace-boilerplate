<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%#= link_to "#", class: "navbar-brand" do %>
    <%#= image_tag "https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/logo.png" %>
    <%# end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
        <li class="nav-item" data-toggle="modal" data-target="#exampleModal">
          <%= link_to "Schedule", "#", class: "navbar-link" %>
        </li>
        <li class="nav-item">
          <%= link_to "Contact", "#", class: "navbar-link" %>
        </li>
        <li class="nav-item">
          <%= link_to "About", "#", class: "navbar-link" %>
        </li>

        <% if current_user %>
          <li class="nav-item">
            <%= link_to "Log Out", destroy_user_session_path, class: 'navbar-link' %>
          </li>
        <% else %>
          <li class="nav-item">
            <%= link_to "Login", new_user_session_path, class: "navbar-link" %>
          </li>
        <% end %>
    </ul>
  </div>
</div>
